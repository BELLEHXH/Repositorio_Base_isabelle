nome = (input("qual e o seu nome? \n"))
peso = float(input("qual e o seu peso ?{nome} \n"))
altura = float(input("qual e a sua altura?{nome}\n"))
imc = peso/(altura*altura)
if imc>18.5:
    print (f"{nome} esta com o imc de {imc} e  esta abaixo do peso")
elif imc >18.5 and  24.9:
    print (f"{nome} esta com imc de {imc} e esta com o peso normal")
elif imc <= 25.0 and 29.9:
    print (f"{nome} esta com imc de {imc} e esta acima do peso")
elif imc <= 30.0 and 34.9:
    print (f"{nome} esta com imc de {imc} e esta com obesidade grau I")
    if imc >=30.0:
       print  (f"{nome} tome cuidado !")
    else:
        print (f" {nome} esta tudo norma")
elif imc <= 35.5 and 39.9:
    print (f"{nome} ,esta com o imc de {imc}e com obesidade grau II!")
else:
     print (f"{nome}esta com imc de {imc} e esta com obesidade grau III")
 

 
